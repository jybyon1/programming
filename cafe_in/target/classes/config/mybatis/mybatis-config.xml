<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.MemberVo" alias="member"/>
		<typeAlias type="vo.ReviewVo" alias="review"/>
		<typeAlias type="vo.FavoritesVo" alias="favorites"/>
		<typeAlias type="vo.ThumbVo"  alias="thumb"/>
		<typeAlias type="vo.CafeVo" alias="cafe"/>
		<typeAlias type="vo.BoardVo"     alias="board"/>
		<typeAlias type="vo.CommentVo"   alias="comment"/>
		<typeAlias type="vo.RecommendVo" alias="recommend"/>
		<typeAlias type="vo.MoodVo" alias="mood"/>
		<typeAlias type="vo.DessertVo" alias="dessert"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/member.xml" />
		<mapper resource="config/mybatis/mapper/review.xml" />
		<mapper resource="config/mybatis/mapper/favorites.xml" />
		<mapper resource="config/mybatis/mapper/board.xml" />
		<mapper resource="config/mybatis/mapper/comment.xml" />
		<mapper resource="config/mybatis/mapper/recommend.xml" />
		<mapper resource="config/mybatis/mapper/mood.xml" />
		<mapper resource="config/mybatis/mapper/dessert.xml" />
		<mapper resource="config/mybatis/mapper/etc.xml" />
		<mapper resource="config/mybatis/mapper/detail.xml" />
		<mapper resource="config/mybatis/mapper/filter.xml" />
		
	</mappers>
</configuration>